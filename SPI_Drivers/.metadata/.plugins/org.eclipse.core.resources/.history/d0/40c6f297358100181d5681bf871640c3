#include "hal_spi_driver.h"
#include "stm32f4xx_gpio_driver.h"
#include "spi_main.h"

/*Configuring for SPI2 */

void spi_gpio_init(void){
	GPIO_PIN_CONFIG spi_conf;

	_HAL_RCC_SPI2_CLK_ENABLE();

	spi_conf.pin = SPI_CLK_PIN;
	//spi_conf.mode =




}


